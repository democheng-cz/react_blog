{
	"compilerOptions": {
		"baseUrl": "./",
		"target": "es5",
		"module": "esnext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"jsx": "react-jsx",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "Node",
		"paths": {
			"@/*": ["src/*"]
		}
	}
}
